    "octokit": "^2.0.7",
    "probot": "^12.2.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/jest": "^29.0.3",
    "@types/node": "^14.14.19",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "smee-client": "^1.2.3",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3",
    "vercel": "^28.2.5"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx, json}": [
      "prettier --write"
