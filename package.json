{
  "name": "gitpoap-bot",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app",
  "author": "Jay Puntham-Baker <jay@gitpoap.io>",
  "license": "ISC",
  "homepage": "https://github.com/gitpoap/gitpoap-bot",
  "keywords": [
    "probot",
    "github",
    "probot-app"
